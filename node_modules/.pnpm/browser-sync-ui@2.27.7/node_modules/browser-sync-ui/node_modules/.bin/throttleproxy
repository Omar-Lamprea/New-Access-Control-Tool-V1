#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/stream-throttle@0.1.3/node_modules/stream-throttle/bin/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/stream-throttle@0.1.3/node_modules/stream-throttle/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/stream-throttle@0.1.3/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules:/Users/omar/Dev/trabajo/AeroNex/node_modules:/Users/omar/Dev/trabajo/node_modules:/Users/omar/Dev/node_modules:/Users/omar/node_modules:/Users/node_modules:/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/browser-sync-ui@2.27.7/node_modules/browser-sync-ui/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/browser-sync-ui@2.27.7/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/stream-throttle@0.1.3/node_modules/stream-throttle/bin/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/stream-throttle@0.1.3/node_modules/stream-throttle/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/stream-throttle@0.1.3/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules:/Users/omar/Dev/trabajo/AeroNex/node_modules:/Users/omar/Dev/trabajo/node_modules:/Users/omar/Dev/node_modules:/Users/omar/node_modules:/Users/node_modules:/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/browser-sync-ui@2.27.7/node_modules/browser-sync-ui/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/browser-sync-ui@2.27.7/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../stream-throttle@0.1.3/node_modules/stream-throttle/bin/throttleproxy.js" "$@"
else
  exec node  "$basedir/../../../../../stream-throttle@0.1.3/node_modules/stream-throttle/bin/throttleproxy.js" "$@"
fi
