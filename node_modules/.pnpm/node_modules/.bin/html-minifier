#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/omar/Dev/Trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/html-minifier@3.5.21/node_modules/html-minifier/node_modules:/Users/omar/Dev/Trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/html-minifier@3.5.21/node_modules:/Users/omar/Dev/Trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/node_modules:/Users/omar/Dev/Trabajo/AeroNex/Nuevo-Front/node_modules:/Users/omar/Dev/Trabajo/AeroNex/node_modules:/Users/omar/Dev/Trabajo/node_modules:/Users/omar/Dev/node_modules:/Users/omar/node_modules:/Users/node_modules:/node_modules:/Users/omar/Dev/Trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/node_modules/html-minifier/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/omar/Dev/Trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/html-minifier@3.5.21/node_modules/html-minifier/node_modules:/Users/omar/Dev/Trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/html-minifier@3.5.21/node_modules:/Users/omar/Dev/Trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/node_modules:/Users/omar/Dev/Trabajo/AeroNex/Nuevo-Front/node_modules:/Users/omar/Dev/Trabajo/AeroNex/node_modules:/Users/omar/Dev/Trabajo/node_modules:/Users/omar/Dev/node_modules:/Users/omar/node_modules:/Users/node_modules:/node_modules:/Users/omar/Dev/Trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/node_modules/html-minifier/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../html-minifier/cli.js" "$@"
else
  exec node  "$basedir/../html-minifier/cli.js" "$@"
fi
