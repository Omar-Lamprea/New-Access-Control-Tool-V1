#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/gifsicle@4.0.1/node_modules/gifsicle/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/gifsicle@4.0.1/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules:/Users/omar/Dev/trabajo/AeroNex/node_modules:/Users/omar/Dev/trabajo/node_modules:/Users/omar/Dev/node_modules:/Users/omar/node_modules:/Users/node_modules:/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/imagemin-gifsicle@6.0.1/node_modules/imagemin-gifsicle/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/imagemin-gifsicle@6.0.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/gifsicle@4.0.1/node_modules/gifsicle/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/gifsicle@4.0.1/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules:/Users/omar/Dev/trabajo/AeroNex/node_modules:/Users/omar/Dev/trabajo/node_modules:/Users/omar/Dev/node_modules:/Users/omar/node_modules:/Users/node_modules:/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/imagemin-gifsicle@6.0.1/node_modules/imagemin-gifsicle/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/imagemin-gifsicle@6.0.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../gifsicle@4.0.1/node_modules/gifsicle/cli.js" "$@"
else
  exec node  "$basedir/../../../../../gifsicle@4.0.1/node_modules/gifsicle/cli.js" "$@"
fi
