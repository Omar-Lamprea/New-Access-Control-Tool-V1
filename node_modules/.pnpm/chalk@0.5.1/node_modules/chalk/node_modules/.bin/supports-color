#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/supports-color@0.2.0/node_modules/supports-color/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/supports-color@0.2.0/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules:/Users/omar/Dev/trabajo/AeroNex/node_modules:/Users/omar/Dev/trabajo/node_modules:/Users/omar/Dev/node_modules:/Users/omar/node_modules:/Users/node_modules:/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/chalk@0.5.1/node_modules/chalk/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/chalk@0.5.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/supports-color@0.2.0/node_modules/supports-color/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/supports-color@0.2.0/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules:/Users/omar/Dev/trabajo/AeroNex/node_modules:/Users/omar/Dev/trabajo/node_modules:/Users/omar/Dev/node_modules:/Users/omar/node_modules:/Users/node_modules:/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/chalk@0.5.1/node_modules/chalk/node_modules:/Users/omar/Dev/trabajo/AeroNex/Nuevo-Front/node_modules/.pnpm/chalk@0.5.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../supports-color@0.2.0/node_modules/supports-color/cli.js" "$@"
else
  exec node  "$basedir/../../../../../supports-color@0.2.0/node_modules/supports-color/cli.js" "$@"
fi
